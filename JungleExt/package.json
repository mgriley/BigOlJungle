{
  "name": "jungleext",
  "version": "1.0.0",
  "description": "Extension that adds support for JungleReader. Needed to support cross-domain web pages.",
  "main": "background.js",
  "scripts": {
    "lint": "web-ext lint",
    "dev": "web-ext lint && web-ext run",
    "build-firefox": "web-ext lint && ./PackageUtil.py --platform firefox",
    "build-firefox-webext": "web-ext lint && web-ext build --overwrite-dest --artifacts-dir firefox-artifacts",
    "build-chrome": "./PackageUtil.py --platform chrome",
    "build-safari": "./PackageUtil.py --platform safari",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Matthew Riley",
  "devDependencies": {
    "web-ext": "^7.6.2"
  }
}
